<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OrderListItemStyle">
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_height">@dimen/ranking_item_height</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:textSize">@dimen/list_item_text_size</item>
    </style>

    <style name="MyOrderBtnStyle">
        <item name="android:layout_width">56dp</item>
        <item name="android:layout_height">24dp</item>
    </style>

    <style name="CongratulationImage">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

</resources>